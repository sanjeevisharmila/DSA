class Solution {
    public ListNode reverseList(ListNode head) {
        ListNode prev=null;
        ListNode current=head;
        while(current!=null){
            ListNode next=current.next;
            current.next=prev;
            prev=current;
            current=next;

        }
        return prev;
    }
}

time=O(n)
space=O(1)

class Solution {
    public ListNode reverseList(ListNode head) {
        if(head==null || head.next==null)
        return head;
        ListNode ans=reverseList(head.next);
        head.next.next=head;
        head.next=null;

        return ans;
    }
}

time=O(n)
space=O(n)
